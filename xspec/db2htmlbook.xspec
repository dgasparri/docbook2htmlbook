    <!-- Table Handling Here 
    
    
When there is a table with title
When there is a table title that has a role


When there is a table with role attibutes
When there is a table with orient attibutes
When there is a table with tabstyle attibutes


When there is a table with no title

When there is a tgroup with a thead and tbody
When there is a tgroup with a thead tbody and tfoot
When there is a tgroup with a tbody and tfoot
When there is a tgroup with a tbody

When there is a row

When there is a entry in a thead
When there is a entry in a tbody
When there is a entry in a tfoot

When an entry has inline content
When an entry has block markup

When there is a colspec
When there is a col

FOR HTML TABLES:

When there is a th
When there is a td
When there is a tr
    
    -->
    
    <x:scenario label="When there is a table with title">
        <x:context>
            <table id="my_table"><title>This is made of cherry wood!</title></table>
        </x:context>
        <x:expect label="it turns into a table with a caption">
            <table id="my_table" xmlns="http://www.w3.org/1999/xhtml"><caption xmlns="http://www.w3.org/1999/xhtml">This is made of cherry wood!</caption></table>
        </x:expect>
    </x:scenario>
    
    <x:scenario label="When there is a table with a title that has a role">
        <x:context>
            <table id="my_table">
                <title role="for_dinner">This is made of cherry wood!</title>
                ...
            </table>
        </x:context>
        <x:expect label="the title's role ends up as a class on the caption">
            <h:table id="my_table">
                <h:caption class="for_dinner">This is made of cherry wood!</h:caption>
                ...
            </h:table>
        </x:expect>
    </x:scenario>
    
    <x:scenario label="When there is a table with a role attibute">
